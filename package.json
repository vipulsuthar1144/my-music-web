{
  "name": "my-music-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "env-cmd -f .env vite --host 0.0.0.0",
    "start:prod": "env-cmd -f .env.production vite",
    "build": "tsc && env-cmd -f .env vite build",
    "build:prod": "tsc && env-cmd -f .env.production vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "preview:prod": "env-cmd -f .env.production vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.19",
    "@mui/utils": "^5.15.20",
    "@reduxjs/toolkit": "^2.2.5",
    "@types/spotify-web-playback-sdk": "^0.1.19",
    "axios": "^1.7.2",
    "env-cmd": "^10.1.0",
    "framer-motion": "^11.14.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.6",
    "react-error-boundary": "^4.1.0",
    "react-pro-sidebar": "^1.1.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "react-toastify": "^10.0.5",
    "react-top-loading-bar": "^2.3.1"
